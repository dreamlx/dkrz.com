<tr>
  <td><%= user.id %></td>
  <td><%= user.openid %></td>
  <td><%= link_to image_tag(user.avatar_url(:thumb)), user.avatar_url if user.avatar_url %></td>
  <td><%= user.number %></td>
  <td><%= user.name %></td>
  <td><%= user.cell %></td>
  <td><%= user.email %></td>
  <td><%= user.channel %></td>
  <td><%= user.superior_id %></td>
  <td><%= user.total_amount %></td>
  <td><%= user.total_commission %></td>
  <td><%= I18n.l(user.created_at, format: :long) %></td>
  <td><%= link_to t(:edit), edit_admin_user_path(user) %></td>
  <td><%= link_to t(:destroy), admin_user_path(user), method: :delete, data: {confirm: t(:are_you_sure)} %></td>
</tr>