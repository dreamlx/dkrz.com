<div class="col-md-12">
  <h3><%= t(:feedback_list) %></h3>
</div>
<div class="col-md-12">
  <table class="table">
    <thead>
      <tr>
        <th><%= Feedback.human_attribute_name(:id) %></th>
        <th><%= User.human_attribute_name(:name) %></th>
        <th><%= User.human_attribute_name(:cell) %></th>
        <th><%= User.human_attribute_name(:email) %></th>
        <th><%= Feedback.human_attribute_name(:content) %></th>
        <th><%= Feedback.human_attribute_name(:created_at) %></th>
        <th colspan="2"><%= t(:operations) %></th>
      </tr>
    </thead>
    <tbody>
      <%= render @feedbacks %>
    </tbody>
  </table>
</div>